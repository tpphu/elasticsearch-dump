    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/bin/elasticdump",
            "args": [
                "--input",
                "http://test.sendo.vn:3000/",
                "--input-index",
                "sendo_new_filter_left_v8/product",
                "--output",
                "http://elastic:changeme@localhost:9200/",
                "--output-index",
                "sendo_new_filter_left_v8/product",
                "--searchBody",
                "{\"query\":{\"bool\":{\"must\":[{\"range\":{\"Updated_at\":{\"lte\":1554778425}}}]}},\"sort\":[{\"External_id\":\"asc\"}], \"fields\": [\"*\"], \"_source\": true}",
                "--offsetFieldName",
                "External_id",
                "--offset",
                "0",
                "--limit",
                "20",
                "--params",
                "{\"preference\": \"_shards:0\"}",
                "--type",
                "data"
            ]
        }
    ]
}